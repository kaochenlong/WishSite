<%= form_with(model: wish_list) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :publish_date %>
    <%= f.date_field :publish_date %>
  </div>
  <%= f.submit %>
<% end %>
